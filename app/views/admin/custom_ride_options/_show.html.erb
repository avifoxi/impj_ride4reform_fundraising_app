<table class='table'>
	<tr>
		<th>
			Display Name
		</th>
		<td>
			<%= custom_ride_option.display_name %>
		</td>
	</tr>
	<tr>
		<th>
			Description
		</th>
		<td>
			<%= custom_ride_option.description %>
		</td>
	</tr>
	<tr>
		<th>
			Liability Text
		</th>
		<td>
			<%= custom_ride_option.liability_text %>
		</td>
	</tr>
	<tr>
		<th>
			Start Date
		</th>
		<td>
			<%= custom_ride_option.start_date %>
		</td>
	</tr>
	<tr>
		<th>
			End Date
		</th>
		<td>
			<%= custom_ride_option.end_date %>
		</td>
	</tr>
	<tr>
		<th>
			Discount Code
		</th>
