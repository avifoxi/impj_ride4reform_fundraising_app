<h3>Donation <%= @donation.id %> <small><%= link_to 'Edit or Delete', edit_admin_donation_path(@donation) %></small> </h3>
<%= render 'donations/stats' %>
<h4>Receipt Info</h4>
<% if @receipt %>
	<%= render 'admin/receipts/stats' %>
<% else %>
	<div>
		Error with receipt for this donation.
	</div>
<% end %>