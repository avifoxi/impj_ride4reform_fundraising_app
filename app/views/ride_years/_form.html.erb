<%= form_for @ride_year do |f| %>
  <%= f.label :registration_fee %>:
  <%= f.text_field :registration_fee %><br />


	<%= f.label :registration_fee_early %>:
  <%= f.text_field :registration_fee_early %><br />

	<%= f.label :min_fundraising_goal %>:
  <%= f.text_field :min_fundraising_goal %><br />

